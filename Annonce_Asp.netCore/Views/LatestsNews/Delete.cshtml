@model Annonce_Asp.netCore.Models.Latest_News

@{
    ViewData["Title"] = "Delete";
}



<h3 class="alert-danger" style="padding:10px">Are you sure you want to delete this?<i class="fa fa-info-circle" style="margin-left:50%"></i></h3>
<div>
    <hr />

    <dl class="dl-horizontal">
        <dt>
            Vidéo :
        </dt>
        <dd>
            <iframe src="@Model.url" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Titre) :
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Titre)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Description) :
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Image) :
        </dt>
        <dd>
            <img src="~/Image_News/@Model.Image" style="width:500px;height:300px" />
        </dd>
        <dt>
            Date Publicité :
        </dt>
        <dd>
            @Html.DisplayFor(model => model.date_Publiciter)
        </dd>
    </dl>
</div>
<div>
    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-outline-danger">Back to List</a>
    </form>
</div>
