@model Annonce_Asp.netCore.Models.Ville

@{
    ViewData["Title"] = "Create";
}

<h4>Ville</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ville_Name" class="control-label">Nom Du Ville : </label>
                <input asp-for="ville_Name" class="form-control" />
                <span asp-validation-for="ville_Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-outline-primary">Back to List</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
