@model Annonce_Asp.netCore.ViewModel.Change_Password
    <br />
    <br />
    <br />
<h4>Change Password</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="OldPassword" class="control-label">Password </label>
                <input asp-for="OldPassword" class="form-control" />
                <span asp-validation-for="OldPassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="newPassword" class="control-label">New Password</label>
                <input asp-for="newPassword" class="form-control" />
                <span asp-validation-for="newPassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ComparePassword" class="control-label"></label>
                <input asp-for="ComparePassword" class="form-control" />
                <span asp-validation-for="ComparePassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="genric-btn success circle arrow" />
                <a href="/Home/index" class="genric-btn success-border circle arrow">Back to List</a>

            </div>
        </form>
    </div>
</div>
@section principal{ }
